<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://maven.apache.org/POM/4.0.0"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">

    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>com.garethahealy.jboss-fuse-examples</groupId>
        <artifactId>fabric8-profiles</artifactId>
        <version>1.0.0-SNAPSHOT</version>
        <relativePath>../</relativePath>
    </parent>
    <artifactId>amq-uk-profile</artifactId>
    <packaging>jar</packaging>
    <name>GarethHealy :: JBoss Fuse Examples :: Fabric8 v1 Profiles :: AMQ UK</name>

    <properties>
        <!-- fabric8 deploy profile configuration -->
        <fabric8.profile>com-garethahealy-amq-uk</fabric8.profile>
        <fabric8.parentProfiles>mq-base</fabric8.parentProfiles>
        <fabric8.features>camel-xstream</fabric8.features>
        <fabric8.featureRepos>${camel-features-url}</fabric8.featureRepos>
        <fabric8.ignoreProject>false</fabric8.ignoreProject>
    </properties>
</project>
